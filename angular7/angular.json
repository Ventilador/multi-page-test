{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "first-dashboard": {
      "root": "first-dashboard",
      "sourceRoot": "first-dashboard/src",
      "projectType": "library",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "first-dashboard/tsconfig.lib.json",
            "project": "first-dashboard/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "first-dashboard/src/test.ts",
            "tsConfig": "first-dashboard/tsconfig.spec.json",
            "karmaConfig": "first-dashboard/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "first-dashboard/tsconfig.lib.json",
              "first-dashboard/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "second-dashboard": {
      "root": "second-dashboard",
      "sourceRoot": "second-dashboard/src",
      "projectType": "library",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "second-dashboard/tsconfig.lib.json",
            "project": "second-dashboard/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "second-dashboard/src/test.ts",
            "tsConfig": "second-dashboard/tsconfig.spec.json",
            "karmaConfig": "second-dashboard/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "second-dashboard/tsconfig.lib.json",
              "second-dashboard/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "workbench": {
      "root": "workbench",
      "sourceRoot": "workbench/src",
      "projectType": "library",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "workbench/tsconfig.lib.json",
            "project": "workbench/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "workbench/src/test.ts",
            "tsConfig": "workbench/tsconfig.spec.json",
            "karmaConfig": "workbench/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "workbench/tsconfig.lib.json",
              "workbench/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "chart": {
      "root": "chart",
      "sourceRoot": "chart/src",
      "projectType": "library",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "chart/tsconfig.lib.json",
            "project": "chart/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "chart/src/test.ts",
            "tsConfig": "chart/tsconfig.spec.json",
            "karmaConfig": "chart/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "chart/tsconfig.lib.json",
              "chart/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "first-dashboard"
}